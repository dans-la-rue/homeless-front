{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "outDir": "../out-tsc/app",
    "types": [],
    "paths": {
      "@modules/*" : ["src/app/Modules/*"],
      "@core/*" : ["src/app/Core/*"],
      "@shared/*" : ["src/app/Shared/*"],
      "@scss/*" : ["src/assets/scss/*"]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
